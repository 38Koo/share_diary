{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": ["jsx-a11y", "tailwindcss"],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
